.lifestyle {
  height: 118rpx;
  width: 700rpx;
  border-radius: 10rpx;
  position: fixed;
  bottom: 40rpx;
  left: 20rpx;
  margin: 0 auto;
  background-color: #535353;
}

lifestyle {
  height: 100%;
  width: 100%;
  background-color: #535353;
  color: #fff;
  border-radius: 10rpx;
}


