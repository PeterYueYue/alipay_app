.page {
  width: 100%;
  min-height: 100vh;
  background: #ffffff;
  box-sizing: border-box;
  padding-bottom: 56rpx;
  text-align: center;
}

.bag {
  width: 100%;
  text-align: center;
}

.bag image {
  width: 596rpx;
  margin-top: 18rpx;
}


.thanks {
  font-size: 32rpx;
  color: rgba(11, 138, 255, 1);
  margin-top: 60rpx;
}
.list {
  font-size:26rpx;
  color:rgba(51,51,51,1);
