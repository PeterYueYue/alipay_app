  text-align: left;
  text-indent: 30%;
  margin-top: 37rpx;
}
.list image {
  width: 22rpx;
  margin-right: 28rpx;
}
.open {
  width: 551rpx;
  height: 120rpx;
  margin: 86rpx auto 0;
  background: url('http://sbag-small.oss-cn-huhehaote.aliyuncs.com/upload/img/web/yy/member/open.png') no-repeat;
  background-size: 100% 100%;
  border: none;
