page {
  background-color: #fff;
  width: 100%;
  height: 100%;
  position: relative;
}

.bgc {
  width: 750rpx;
}

.top {
  position: relative;
  color: #fff;
}

.num {
  position: absolute;
  left: 0;
  top: 172rpx;
  right: 0;
  margin: auto;
  text-align: center;
}

.num-num {
  font-size: 54rpx;
}

.num-text {
  font-size: 26rpx;
}

.earnings {
  position: absolute;
  top: 334rpx;
