}

.hint {
  border-bottom: 20rpx solid #fafafa;
}

.hint-img {
  width: 26rpx;
  margin: 0 18rpx 0 34rpx;
  vertical-align: top;
}

.hint-text {
  font-size: 24rpx;
  color: #808080;
  width: 640rpx;
  display: inline-block;
  margin-bottom: 32rpx;
}

.money {
  font-size: 28rpx;
